nt_A_Apres": "A", "divisionRattachee": "ARTICLE 42", "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0324C.asp#D_Etat_B"}, "missionVisee": {"codeEtat": "B", "codeMissionMinefi": "RA", "libelleMission": "Recherche et enseignement sup\u00e9rieur", "missionRef": "2886"}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositifAmdtCreditPLF": {"listeProgrammes": {"programme": [{"libelle": "Formations sup\u00e9rieures et recherche universitaire", "programmeRef": "397050", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Vie \u00e9tudiante", "programmeRef": "397052", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Recherches scientifiques et technologiques pluridisciplinaires", "programmeRef": "397054", "autorisationEngagement": "-58965406", "creditPaiement": "-58965406", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Recherche spatiale", "programmeRef": "397056", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Recherche dans les domaines de l'\u00e9nergie, du d\u00e9veloppement et de la mobilit\u00e9 durables", "programmeRef": "397058", "autorisationEngagement": "58965406", "creditPaiement": "58965406", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Recherche et enseignement sup\u00e9rieur en mati\u00e8re \u00e9conomique et industrielle", "programmeRef": "397060", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Recherche duale (civile et militaire)", "programmeRef": "397062", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Enseignement sup\u00e9rieur et recherche agricoles", "programmeRef": "397064", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;amendement vise &#x00E0; compenser la baisse de -10,56&nbsp;% des cr&#x00E9;dits de la sous-action 16.01&nbsp;&#x2013;&nbsp;Soutien aux activit&#x00E9;s nucl&#x00E9;aires du CEA, de l&#x2019;action , du programme 190&nbsp;&#x2013;&nbsp;Recherche dans les domaines de l&#x2019;&#x00E9;nergie, du d&#x00E9;veloppement et de la mobilit&#x00E9; durables, soit une r&#x00E9;duction de 58 965 406&nbsp;euros (passant de 558 293 105&nbsp;euros en 2024 &#x00E0; 499 327 699&nbsp;euros dans le PLF 2025).</p><p style=\"text-align: justify;\">Pour ce faire, l&#x2019;amendement propose&nbsp;:</p><p style=\"text-align: justify;\">Un transfert de cr&#x00E9;dits de 58 965 406&nbsp;euros en autorisations d&#x2019;engagement et en cr&#x00E9;dits de paiement depuis l&#x2019;action 01&nbsp;&#x2013;&nbsp;Pilotage et animation, du Programme 172&nbsp;: Recherches scientifiques et technologiques pluridisciplinaires dont 5 millions d&#x2019;euros pris de la sous-action 8&nbsp;&#x2013;&nbsp;Renforcement des liens entre science et soci&#x00E9;t&#x00E9;, en ciblant plus particuli&#x00E8;rement le label SAPS dont les r&#x00E9;sultats sont mitig&#x00E9;s, vers le programme 190.</p><p style=\"text-align: justify;\"><br>La recherche nucl&#x00E9;aire est essentielle pour garantir la souverainet&#x00E9; &#x00E9;nerg&#x00E9;tique de la France et pour contribuer &#x00E0; la lutte contre le r&#x00E9;chauffement climatique. La r&#x00E9;duction des cr&#x00E9;dits dans ce domaine strat&#x00E9;gique pourrait affaiblir les efforts fran&#x00E7;ais en mati&#x00E8;re de recherche et d&#x00E9;veloppement dans le nucl&#x00E9;aire, un secteur cl&#x00E9; pour l&#x2019;avenir &#x00E9;nerg&#x00E9;tique bas-carbone du pays. Par cons&#x00E9;quent, cet amendement propose de r&#x00E9;affecter les cr&#x00E9;dits d&#x2019;un autre programme moins prioritaire et strat&#x00E9;gique afin de maintenir les investissements cruciaux dans le secteur de la recherche nucl&#x00E9;aire, d&#x2019;annuler la diminution de la subvention au titre du financement du projet &#x00AB;&nbsp;R&#x00E9;acteur Jules Horowitz&nbsp;&#x00BB; et remettre la subvention au titre du financement de la R&amp;amp;D dans le domaine nucl&#x00E9;aire au niveau initialement pr&#x00E9;vu.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-19", "datePublication": "2024-11-15", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-10-23T22:09:10+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419604B0324P2D1N000264?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}