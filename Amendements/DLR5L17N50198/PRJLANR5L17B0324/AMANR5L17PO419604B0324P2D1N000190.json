isee": {"codeEtat": "B", "codeMissionMinefi": "CB", "libelleMission": "Culture", "missionRef": "2856"}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositifAmdtCreditPLF": {"listeProgrammes": {"programme": [{"libelle": "Patrimoines", "programmeRef": "391714", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Cr\u00e9ation", "programmeRef": "391716", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Transmission des savoirs et d\u00e9mocratisation de la culture", "programmeRef": "391718", "autorisationEngagement": "0", "creditPaiement": "0", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Soutien aux politiques du minist\u00e8re de la culture", "programmeRef": "391720", "autorisationEngagement": "-1500000", "creditPaiement": "-1500000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "-1500000", "soldeCP": "-1500000", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\"><i>Amendement de repli</i>.</p><p style=\"text-align: justify;\">Cet amendement a pour objectif de r&#x00E9;aliser 1&nbsp;500 000&nbsp;euros d&#x2019;&#x00E9;conomies sur le budget 2025 du Minist&#x00E8;re de la Culture.</p><p style=\"text-align: justify;\">La France traverse une p&#x00E9;riode de contraintes budg&#x00E9;taires o&#x00F9; chaque minist&#x00E8;re est appel&#x00E9; &#x00E0; faire des &#x00E9;conomies. Le budget de la Culture pour 2025 ne refl&#x00E8;te pas suffisamment cette exigence.&nbsp;</p><p style=\"text-align: justify;\">Pour contribuer &#x00E0; l&#x2019;effort national, il est crucial de r&#x00E9;aliser des &#x00E9;conomies structurelles l&#x00E0; o&#x00F9; c&#x2019;est possible, en rationalisant les d&#x00E9;penses.</p><p style=\"text-align: justify;\">Aussi, convient-il de r&#x00E9;duire les autorisations d&#x2019;engagement et cr&#x00E9;dits de paiement pr&#x00E9;vus &#x00E0; l&#x2019;action 06 Action culturelle internationale du programme 224 &#x00AB;&nbsp;Soutien aux politiques du minist&#x00E8;re de la culture&nbsp;&#x00BB; en fixant comme montant celui du PLF2024 (&#x00E0; d&#x00E9;faut de retenir celui du PLF2023), soit 9 974 895&nbsp;euros.</p><p style=\"text-align: justify;\">Cet amendement propose&nbsp;:</p><p style=\"text-align: justify;\">- de r&#x00E9;duire de 1,5 M&#x20AC; les&nbsp;AE et CP &#x00E0; l&#x2019;action 06 &#x00AB;&nbsp;Action culturelle internationale&nbsp;&#x00BB; du programme 224&nbsp;&#x00AB;&nbsp;Soutien aux politiques du minist&#x00E8;re de la culture&nbsp;&#x00BB;.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-10-18", "datePublication": "2024-11-15", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-10-23T09:40:57+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419604B0324P2D1N000190?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "26274", "typePosition": "Dernier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}