{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901B0324P2D1N003629", "chronotag": "1739874651968", "legislature": "17", "identification": {"numeroLong": "II-3629", "numeroOrdreDepot": "3629", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901B0324P2D1", "texteLegislatifRef": "PRJLANR5L17B0324", "triAmendement": "ctiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaazzw", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "Rapporteur", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794114", "groupePolitiqueRef": "PO845454", "auteurRapporteurOrganeRef": "PO59048"}, "cosignataires": {"acteurRef": ["PA719138", "PA607619", "PA822617", "PA267763"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Mandon, rapporteur sp&#233;cial au nom de la commission des finances,  M.&#160;Castellani, M.&#160;Molac, Mme&#160;Froger et M.&#160;Bataille"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "\u00c9TAT B", "articleDesignationCourte": "ART. 42", "articleDesignation": "ARTICLE 42", "type": "ANNEXE", "avant_A_Apres": "A", "divisionRattachee": "ARTICLE 42", "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0324C.asp#D_Etat_B"}, "missionVisee": {"codeEtat": "B", "codeMissionMinefi": "RC", "libelleMission": "Relations avec les collectivit\u00e9s territoriales", "missionRef": "2890"}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositifAmdtCreditPLF": {"listeProgrammes": {"programme": [{"libelle": "Concours financiers aux collectivit\u00e9s territoriales et \u00e0 leurs groupements", "programmeRef": "469604", "autorisationEngagement": "50000000", "creditPaiement": "50000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, {"libelle": "Concours sp\u00e9cifiques et administration", "programmeRef": "469606", "autorisationEngagement": "-50000000", "creditPaiement": "-50000000", "action": "modification", "lignesCredits": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}]}, "soldeAE": "0", "soldeCP": "0", "cpEgalAe": "true"}, "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; octroyer une dotation exceptionnelle de continuit&#x00E9; territoriale de 50 millions d&#x2019;euros &#x00E0; la collectivit&#x00E9; de Corse afin de ne pas mettre &#x00E0; mal le service public de transport a&#x00E9;rien et maritime de l&#x2019;&#x00EE;le. Pour rappel, La dotation de continuit&#x00E9; territoriale (DCT) n&#x2019;est plus index&#x00E9;e sur l&#x2019;inflation depuis 2009.</p><p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement effectue les mouvements de cr&#x00E9;dits suivants&nbsp;:</p><p style=\"text-align: justify;\">&#x2013;&nbsp;augmentation de 50M&#x20AC; d&#x2019;euros en AE et CP sur l&#x2019;action 05 du Programme 119 qui comprend les cr&#x00E9;dits de la DCT&nbsp;;</p><p style=\"text-align: justify;\">&#x2013;&nbsp;diminution de 50M&#x20AC; en AE et CP de l&#x2019;action 01 du programme 122. Il est demand&#x00E9; au Gouvernement de lever le gage.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-11-09", "datePublication": "2024-11-15", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "RT", "libelle": "Retir\u00e9"}, "sousEtat": {"code": "RT", "libelle": "Retir\u00e9 apr\u00e8s publication"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901B0324P2D1N003629?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}