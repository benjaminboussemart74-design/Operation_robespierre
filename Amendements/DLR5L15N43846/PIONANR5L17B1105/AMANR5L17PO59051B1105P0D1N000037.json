onTexteVise": "/17/textes/1105.asp#D_Article_1er_ter"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "6", "alineaDesignation": "Alin\u00e9a 6"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter&nbsp;l&#x2019;alin&#x00E9;a&nbsp;6&nbsp;par les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;et, au d&#x00E9;but de la seconde phrase du m&#x00EA;me alin&#x00E9;a, sont ins&#x00E9;r&#x00E9;s les mots&nbsp;: &#x00AB;&nbsp;Dans les communes de 1 000 habitants et plus,&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Amendement d'appel</p><p>Cet amendement vise &#x00E0; maintenir la possibilit&#x00E9; pour les communes de moins de 1 000 habitants d'&#x00EA;tre dot&#x00E9;es de listes qui sont l'expression de la volont&#x00E9; populaire autant que de la volont&#x00E9; d'engagement des citoyens.</p><p>Les &#x00E9;lus des petites communes doivent continuer &#x00E0; &#x00EA;tre &#x00E9;lus comme adjoints par leurs concitoyens selon leurs volont&#x00E9;s et comp&#x00E9;tences, non en fonction de quota.</p><p>Il en va de la libert&#x00E9; des &#x00E9;lus &#x00E0; choisir leur engagement au service de leur communaut&#x00E9;.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-28", "datePublication": "2025-04-01", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-04-02T10:31:35+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59051B1105P0D1N000037?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "8", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "29678", "typePosition": "Dernier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}