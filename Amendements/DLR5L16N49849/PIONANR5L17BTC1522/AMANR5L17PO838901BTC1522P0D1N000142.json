{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1522P0D1N000142", "chronotag": "1757950188861", "legislature": "17", "identification": {"numeroLong": "142", "numeroOrdreDepot": "142", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1522P0D1", "texteLegislatifRef": "PIONANR5L17BTC1522", "triAmendement": "fsaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaae", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA719890", "groupePolitiqueRef": "PO845407", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Brulebois"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 16 bis", "articleDesignationCourte": "ART. 16 BIS", "articleDesignation": "ARTICLE 16 BIS", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1522.asp#D_Articles_14_15_16_et_16_bis"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;tablir cet article dans la r&#x00E9;daction suivante&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Le deuxi&#x00E8;me alin&#x00E9;a de l&#x2019;article&nbsp;L.&nbsp;542&#x2011;13&#x2011;2 du code de l&#x2019;environnement est ainsi modifi&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;1&#x00B0;&nbsp;Apr&#x00E8;s la premi&#x00E8;re phrase, est ins&#x00E9;r&#x00E9;e une phrase ainsi r&#x00E9;dig&#x00E9;e&nbsp;: &#x00AB;&nbsp;L&#x2019;autorit&#x00E9; administrative peut &#x00E9;galement requalifier ces mati&#x00E8;res radioactives en stock strat&#x00E9;gique quand existent des perspectives de valorisation dont l&#x2019;op&#x00E9;rabilit&#x00E9; n&#x2019;est pas encore &#x00E9;tablie.&nbsp;&#x00BB;&nbsp;;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;2&#x00B0;&nbsp;&#x00C0; la seconde phrase, les mots&nbsp;: &#x00AB;&nbsp;cette requalification&nbsp;&#x00BB; sont remplac&#x00E9;s par les mots&nbsp;: &#x00AB;&nbsp;ces requalifications&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement propose de r&#x00E9;tablir l'article 16 bis dans sa r&#x00E9;daction issue du S&#x00E9;nat. Il vise &#x00E0; permettre &#x00E0; l'autorit&#x00E9; administrative de requalifier des&nbsp;mati&#x00E8;res radioactives en stock strat&#x00E9;gique quand existent des perspectives de valorisation dont l&#x2019;op&#x00E9;rabilit&#x00E9; n&#x2019;est pas encore &#x00E9;tablie.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-06-11", "datePublication": "2025-06-19", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2025-06-19T23:11:53+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1522P0D1N000142?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29571", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "31916", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "80080", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}