{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1522P0D1N000052", "chronotag": "1757950184489", "legislature": "17", "identification": {"numeroLong": "52", "numeroOrdreDepot": "52", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1522P0D1", "texteLegislatifRef": "PIONANR5L17BTC1522", "triAmendement": "bwaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabcp", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794178", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA266793", "PA722390", "PA840979", "PA330909", "PA719968", "PA718850", "PA607090", "PA794750", "PA718884", "PA642725", "PA721816", "PA794042", "PA342240"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Taite,  M.&#160;Bony, Mme&#160;Corneloup, M.&#160;Le&#160;Fur, M.&#160;Descoeur, Mme&#160;Sylvie&#160;Bonnet, M.&#160;Cordier, M.&#160;Jean-Pierre&#160;Vigier, M.&#160;Liger, Mme&#160;Bazin-Malgras, M.&#160;Li&#233;geon, M.&#160;Boucard, Mme&#160;Dezarnaud et M.&#160;Bourgeaux"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 9", "articleDesignationCourte": "ART. 9", "articleDesignation": "ARTICLE 9", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1522.asp#D_Article_9"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;l&#x2019;alin&#x00E9;a&nbsp;2, apr&#x00E8;s la premi&#x00E8;re occurrence du mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;habitation&nbsp;&#x00BB;,&nbsp;</p><p style=\"text-align: justify;\">ins&#x00E9;rer les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;en prenant en compte les besoins et sp&#x00E9;cificit&#x00E9;s de r&#x00E9;novation thermiques des b&#x00E2;timents ruraux&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Les b&#x00E2;timents situ&#x00E9;s en milieu rural pr&#x00E9;sentent des sp&#x00E9;cificit&#x00E9;s en mati&#x00E8;re de r&#x00E9;novation &#x00E9;nerg&#x00E9;tique : il s&#x2019;agit majoritairement de maisons anciennes, individuelles, de grande superficie, et avec des contraintes techniques qui complexifient les op&#x00E9;rations de r&#x00E9;novation. Ces caract&#x00E9;ristiques sont tr&#x00E8;s diff&#x00E9;rentes de celles des b&#x00E2;timents urbains et n&#x00E9;cessitent une approche diff&#x00E9;renci&#x00E9;e pour garantir l&#x2019;efficacit&#x00E9; des politiques publiques en mati&#x00E8;re de r&#x00E9;novation.<br>En pr&#x00E9;cisant que la politique &#x00E9;nerg&#x00E9;tique nationale doit tenir compte des besoins et sp&#x00E9;cificit&#x00E9;s de r&#x00E9;novation thermique des b&#x00E2;timents ruraux, cet amendement vise &#x00E0; rendre plus &#x00E9;quitables les objectifs de performance &#x00E9;nerg&#x00E9;tique en reconnaissant la diversit&#x00E9; des situations territoriales</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-06-11", "datePublication": "2025-06-19", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2025-06-19T19:27:47+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1522P0D1N000052?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29570", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}