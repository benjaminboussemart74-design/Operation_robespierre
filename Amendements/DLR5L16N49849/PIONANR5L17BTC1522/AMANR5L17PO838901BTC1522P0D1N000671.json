#160;Pahun, M.&#160;Fr&#233;d&#233;ric&#160;Petit, Mme&#160;Maud&#160;Petit, Mme&#160;Poueyto, Mme&#160;Thillaye, M.&#160;Turquois et M.&#160;Philippe&#160;Vigier"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 4", "articleDesignationCourte": "APR\u00c8S ART. 4", "articleDesignation": "APR\u00c8S L'ARTICLE 4, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1522.asp#D_Article_4"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Au&nbsp;10&#x00B0; de l&#x2019;article&nbsp;L.&nbsp;100&#x2011;2 du code de l&#x2019;&#x00E9;nergie, apr&#x00E8;s le mot&nbsp;: &#x00AB;&nbsp;&#x00E9;nergie&nbsp;&#x00BB;, sont ins&#x00E9;r&#x00E9;s les mots&nbsp;: &#x00AB;&nbsp;et les usages de la biomasse qui permettent de valoriser ses coproduits,&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement port&#x00E9; par le groupe Les D&#x00E9;mocrates vise &#x00E0; reconna&#x00EE;tre la place vertueuse qu&#x2019;occupent les usages des coproduits de la biomasse foresti&#x00E8;re, dont le chauffage aux granul&#x00E9;s de bois ou aux plaquettes foresti&#x00E8;res, dans le mix &#x00E9;nerg&#x00E9;tique.</p><p style=\"text-align: justify;\">De mani&#x00E8;re sp&#x00E9;cifique, le granul&#x00E9; de bois fran&#x00E7;ais provient en effet &#x00E0; 90&nbsp;% de connexes de scierie (sciures, &#x00E9;corces, etc.) qui valorisent la production de bois d&#x2019;&#x0153;uvre et de bois d&#x2019;industrie, en trouvant de nouveaux d&#x00E9;bouch&#x00E9;s.</p><p style=\"text-align: justify;\">Les 10&nbsp;% restants sont des connexes d&#x2019;exploitation de la for&#x00EA;t (bois malade, bois d&#x2019;&#x00E9;claircie, etc.).</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-06-12", "datePublication": "2025-06-18", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Retir\u00e9"}}, "dateSort": "2025-06-18T21:59:27+02:00", "sort": "Retir\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1522P0D1N000671?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29568", "article99": "false", "loiReference": {"codeLoi": "c \u00e9nergie", "divisionCodeLoi": "L100-2"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}