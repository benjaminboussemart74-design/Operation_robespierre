{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419610B0463P0D1N000247", "chronotag": "1757947403819", "legislature": "17", "identification": {"numeroLong": "CE247", "numeroOrdreDepot": "247", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B0463P0D1", "texteLegislatifRef": "PIONANR5L17B0463", "triAmendement": "duaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaab", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA841315", "groupePolitiqueRef": "PO845419", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA342415", "PA794854", "PA840765", "PA643157", "PA607553", "PA840673", "PA793816"]}, "suffixe": "et les membres du groupe Socialistes et apparent\u00e9s", "libelle": "M.&#160;Benbrahim,  Mme&#160;Battistel, M.&#160;Echaniz, M.&#160;Lhardit, M.&#160;Naillet, M.&#160;Potier, Mme&#160;Rossi, Mme&#160;Thomin et les membres du groupe Socialistes et apparent&#233;s"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 14", "articleDesignationCourte": "ART. 14", "articleDesignation": "ARTICLE 14", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0463.asp#D_Article_14"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer cet article.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement des d&#x00E9;put&#x00E9;s Socialistes et apparent&#x00E9;s vise &#x00E0; supprimer cette disposition qui ne rel&#x00E8;ve pas de dispositions de programmation et qui vise &#x00E0; renforcer certaines mesures de simplification pr&#x00E9;vues par la loi &#x00AB;&nbsp;Nouveau nucl&#x00E9;aire&nbsp;&#x00BB;, du&nbsp;22&nbsp;juin&nbsp;2023.</p><p style=\"text-align: justify;\">En outre, ces dispositions, adopt&#x00E9;es depuis moins de deux ans, n&#x2019;ont pas encore trouv&#x00E9; &#x00E0; s&#x2019;appliquer pleinement et n&#x2019;ont pas fait l&#x2019;objet d&#x2019;une &#x00E9;valuation dans le cadre de l&#x2019;&#x00E9;valuation des lois &#x00E0; trois ans. Il n&#x2019;appara&#x00EE;t donc pas pertinent de les modifier &#x00E0; ce stade.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-28", "datePublication": "2025-06-04", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-06-04T16:30:55+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419610B0463P0D1N000247?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "79518", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}