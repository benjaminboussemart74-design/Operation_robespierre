{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419610B0463P0D1N000509", "chronotag": "1757947388081", "legislature": "17", "identification": {"numeroLong": "CE509", "numeroOrdreDepot": "509", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B0463P0D1", "texteLegislatifRef": "PIONANR5L17B0463", "triAmendement": "daaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaati", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA841495", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793246", "PA794590", "PA841479", "PA719436"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Amblard,  M.&#160;Falcon, M.&#160;Loubet, M.&#160;Weber et M.&#160;Meizonnet"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 13", "articleDesignationCourte": "ART. 13", "articleDesignation": "ARTICLE 13", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0463.asp#D_Article_13"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "4", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 4"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;4, ins&#x00E9;rer les deux alin&#x00E9;as suivants&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;<i>a)</i>&nbsp;<i>bis</i> Le 2&#x00B0; est ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;2&#x00B0;&nbsp;&#x00C0; l&#x2019;am&#x00E9;lioration de l&#x2019;efficacit&#x00E9; &#x00E9;nerg&#x00E9;tique et &#x00E0; la baisse de la consommation d&#x2019;&#x00E9;nergie primaire fossile. Ce volet peut identifier des usages pour lesquels la substitution d&#x2019;une &#x00E9;nergie &#x00E0; une autre est une priorit&#x00E9; et indique des priorit&#x00E9;s de baisse de la consommation d&#x2019;&#x00E9;nergie fossile par type d&#x2019;&#x00E9;nergie, en fonction du facteur d&#x2019;&#x00E9;mission de gaz &#x00E0; effet de serre de chacune. Il identifie les usages pour lesquels l&#x2019;am&#x00E9;lioration de l&#x2019;efficacit&#x00E9; &#x00E9;nerg&#x00E9;tique et la ma&#x00EE;trise de la consommation d&#x2019;&#x00E9;nergie primaire sont une priorit&#x00E9;. Il contient une feuille de route de la r&#x00E9;novation &#x00E9;nerg&#x00E9;tique des b&#x00E2;timents, pr&#x00E9;cisant les modalit&#x00E9;s de mise en &#x0153;uvre de l&#x2019;objectif de r&#x00E9;duction de la consommation &#x00E9;nerg&#x00E9;tique finale mentionn&#x00E9; au&nbsp;2&#x00B0; du&nbsp;I de l&#x2019;article&nbsp;L.&nbsp;100&#x2011;4 du code de l&#x2019;&#x00E9;nergie pour les b&#x00E2;timents &#x00E0; usage r&#x00E9;sidentiel ou tertiaire, et de l&#x2019;objectif de r&#x00E9;novation des b&#x00E2;timents en fonction des normes &#x00AB;&nbsp;b&#x00E2;timent basse consommation&nbsp;&#x00BB; ou assimil&#x00E9;es mentionn&#x00E9;es au&nbsp;7&#x00B0; du m&#x00EA;me I&nbsp;;&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; cibler la baisse volontaire de la consommation d&#x2019;&#x00E9;nergie seulement sur les &#x00E9;nergies fossiles, par sur toute la consommation d&#x2019;&#x00E9;nergie finale.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-28", "datePublication": "2025-06-04", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-06-04T16:13:22+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419610B0463P0D1N000509?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}