DivisionTexteVise": "/17/textes/0463.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "3", "alineaDesignation": "Alin\u00e9a 3"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;diger ainsi l&#x2019;alin&#x00E9;a&nbsp;3&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;3&#x00B0;&nbsp;<i>ter</i> Garantir l&#x2019;existence d&#x2019;un tarif r&#x00E9;glement&#x00E9; de vente de gaz naturel, la d&#x00E9;tention par l&#x2019;&#x00C9;tat d&#x2019;une partie du capital de l&#x2019;entreprise d&#x00E9;nomm&#x00E9;e&nbsp;&#x00BB; Engie &#x00AB;&nbsp;,&nbsp;conform&#x00E9;ment &#x00E0; l&#x2019;article&nbsp;L.&nbsp;111&#x2011;68 du pr&#x00E9;sent code, la propri&#x00E9;t&#x00E9; publique du r&#x00E9;seau de distribution de gaz conform&#x00E9;ment &#x00E0; l&#x2019;article&nbsp;L.&nbsp;432&#x2011;4, la s&#x00E9;curit&#x00E9; d&#x2019;approvisionnement en gaz ainsi que la diversification des importations dans ce secteur&nbsp;;&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Dans un contexte de forte instabilit&#x00E9; g&#x00E9;opolitique et de volatilit&#x00E9; des prix du gaz, cet amendement vise &#x00E0; ajouter la garantie de l&#x2019;existence d&#x2019;un tarif r&#x00E9;glement&#x00E9; assurant une pr&#x00E9;visibilit&#x00E9; pour les m&#x00E9;nages et les industries, face &#x00E0; des march&#x00E9;s d&#x00E9;r&#x00E9;gul&#x00E9;s souvent d&#x00E9;connect&#x00E9;s des r&#x00E9;alit&#x00E9;s de production.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-28", "datePublication": "2025-06-02", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-06-02T22:21:59+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419610B0463P0D1N000472?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}