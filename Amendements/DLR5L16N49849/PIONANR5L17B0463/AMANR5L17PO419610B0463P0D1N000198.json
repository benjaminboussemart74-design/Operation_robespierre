if": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p>&#x00C0;&nbsp;l&#x2019;alin&#x00E9;a&nbsp;16, apr&#x00E8;s la deuxi&#x00E8;me occurrence du mot&nbsp;:&nbsp;</p><p>&#x00AB;&nbsp;installations&nbsp;&#x00BB;,</p><p>&nbsp;ins&#x00E9;rer les mots&nbsp;:&nbsp;</p><p>&nbsp;&#x00AB; , avec un rythme de nouvelles capacit&#x00E9;s install&#x00E9;es de production &#x00E0; 1,5 gigawatts par an jusqu&#x2019;en 2035, &#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement de repli du groupe &#x00C9;cologiste et Social propose d&#x2019;inscrire un rythme d&#x2019;installation de nouvelles capacit&#x00E9;s de production d&#x2019;&#x00E9;olien terrestre d&#x2019;au moins 1,5 GW par an jusqu&#x2019;en 2035, conform&#x00E9;ment aux objectifs de la Strat&#x00E9;gie fran&#x00E7;aise &#x00E9;nergie-climat (SFEC) pr&#x00E9;sent&#x00E9;e par le Gouvernement en 2023.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-28", "datePublication": "2025-06-04", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-06-04T10:55:29+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419610B0463P0D1N000198?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://ww