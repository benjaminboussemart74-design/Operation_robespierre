{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419610B0463P0D1N000465", "chronotag": "1757947351274", "legislature": "17", "identification": {"numeroLong": "CE465", "numeroOrdreDepot": "465", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B0463P0D1", "texteLegislatifRef": "PIONANR5L17B0463", "triAmendement": "acaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaco", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA840817", "groupePolitiqueRef": "PO845485", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA841017", "PA720976", "PA721486"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Bruneau,  M.&#160;Huwart, M.&#160;Mathiasin et M.&#160;Naegelen"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0463.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Avant", "numero": "1", "alineaDesignation": "Avant l'alin\u00e9a 1"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;Avant l&#x2019;alin&#x00E9;a&nbsp;1, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Au&nbsp;2&#x00B0; de l&#x2019;article&nbsp;L.&nbsp;100&#x2011;2 du code de l&#x2019;&#x00E9;nergie, les mots&nbsp;: &#x00AB;&nbsp;&#x00E0; l&#x2019;&#x00E9;nergie&nbsp;&#x00BB; sont remplac&#x00E9;s par les mots&nbsp;:&nbsp;&#x00AB;&nbsp;&#x00E0; l&#x2019;&#x00E9;nergie &#x00E0; un prix raisonnable sans&nbsp;variation&nbsp;brutale&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;Compl&#x00E9;ter cet article par l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;II.&nbsp;&#x2013;&nbsp;La perte de recettes pour l&#x2019;&#x00C9;tat est compens&#x00E9;e &#x00E0; due concurrence par la cr&#x00E9;ation d&#x2019;une taxe additionnelle &#x00E0; l&#x2019;accise sur les tabacs pr&#x00E9;vue au chapitre IV du titre I<sup>er</sup> du livre III du code des impositions sur les biens et services.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La pr&#x00E9;carit&#x00E9; &#x00E9;nerg&#x00E9;tique est provoqu&#x00E9;e par les prix tr&#x00E8;s &#x00E9;lev&#x00E9;s que nous avons pu conna&#x00EE;tre ainsi qu&#x2019;aux variations brutales induites par des &#x00E9;v&#x00E8;nements internationaux impr&#x00E9;visibles mais aussi &#x00E0; cause des sp&#x00E9;culations sur les march&#x00E9;s de l&#x2019;&#x00E9;nergie qui en d&#x00E9;coulent souvent. Pr&#x00E9;server les populations les plus pr&#x00E9;caires de ces &#x00E9;v&#x00E8;nements brutaux semble pouvoir &#x00EA;tre un objectif d&#x2019;une politique &#x00E9;nerg&#x00E9;tique d&#x2019;un pays d&#x00E9;velopp&#x00E9;.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-28", "datePublication": "2025-06-02", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2025-06-02T21:43:06+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419610B0463P0D1N000465?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}