{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1354P0D1N000019", "chronotag": "1757948413292", "legislature": "17", "identification": {"numeroLong": "19", "numeroOrdreDepot": "19", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1354P0D1", "texteLegislatifRef": "PIONANR5L17BTC1354", "triAmendement": "qaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabo", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA718784", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Pauget"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 3", "articleDesignationCourte": "ART. 3", "articleDesignation": "ARTICLE 3", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1354.asp#D_Article_3"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;diger ainsi la deuxi&#x00E8;me phrase de l&#x2019;alin&#x00E9;a&nbsp;2&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Cet examen, visant &#x00E0; d&#x00E9;terminer l&#x2019;aptitude &#x00E0; la conduite du conducteur et r&#x00E9;alis&#x00E9; &#x00E0; ses frais, se tient dans un d&#x00E9;lai de 72 heures &#x00E0; compter de l&#x2019;accident routier ou, le cas &#x00E9;ch&#x00E9;ant, dans un d&#x00E9;lai de 72 heures &#x00E0; compter du moment&nbsp;o&#x00F9; l&#x2019;&#x00E9;tat de sant&#x00E9; du conducteur le permet.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; clarifier la formulation de la deuxi&#x00E8;me phrase du deuxi&#x00E8;me alin&#x00E9;a qui pr&#x00E9;cise le d&#x00E9;lai dans lequel doit &#x00EA;tre r&#x00E9;alis&#x00E9; l&#x2019;examen m&#x00E9;dical pr&#x00E9;vu par le pr&#x00E9;sent article. Il indique en outre que, lorsque l&#x2019;&#x00E9;tat de sant&#x00E9; du conducteur concern&#x00E9; ne permet pas la r&#x00E9;alisation imm&#x00E9;diate dudit examen, celui-ci sera effectu&#x00E9; dans un d&#x00E9;lai de 72 heures &#x00E0; compter du moment o&#x00F9; son &#x00E9;tat de sant&#x00E9; le permettra.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-05-02", "datePublication": "2025-06-03", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-06-03T19:03:15+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1354P0D1N000019?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29499", "article99": "false", "loiReference": {"codeLoi": "2", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "78222", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}