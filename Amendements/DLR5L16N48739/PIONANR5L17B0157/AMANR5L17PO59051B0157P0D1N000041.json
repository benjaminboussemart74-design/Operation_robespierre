itif": "<p style=\"text-align: justify;\">Apr&#x00E8;s le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;chapitre,&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">r&#x00E9;diger ainsi la fin de l&#x2019;alin&#x00E9;a&nbsp;2&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;la dur&#x00E9;e de cette suspension est de dix ans au plus&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement r&#x00E9;tablit le dispositif adopt&#x00E9; par l&#x2019;Assembl&#x00E9;e nationale en premi&#x00E8;re lecture, s&#x2019;agissant de la dur&#x00E9;e maximale de suspension du permis de conduire en cas d&#x2019;atteinte volontaire &#x00E0; l&#x2019;int&#x00E9;grit&#x00E9; physique.</p><p style=\"text-align: justify;\">Ce r&#x00E9;tablissement est par ailleurs coh&#x00E9;rent avec les modifications propos&#x00E9;es &#x00E0; l&#x2019;article&nbsp;1<sup>er</sup>.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-28", "datePublication": "2025-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-04-30T12:11:18+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO59051B0157P0D1N000041?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://ww