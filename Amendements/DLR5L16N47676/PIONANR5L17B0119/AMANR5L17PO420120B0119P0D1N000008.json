{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO420120B0119P0D1N000008", "chronotag": "1757947493873", "legislature": "17", "identification": {"numeroLong": "AS8", "numeroOrdreDepot": "8", "prefixeOrganeExamen": "CION-SOC", "numeroRect": "0"}, "examenRef": "EXANR5L17PO420120B0119P0D1", "texteLegislatifRef": "PIONANR5L17B0119", "triAmendement": "eaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaws", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794678", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA842279", "PA794434", "PA841645", "PA840119", "PA794762", "PA842073", "PA840143", "PA795912", "PA793608", "PA793672", "PA794322", "PA793354", "PA793696", "PA793230", "PA793382"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Frapp&#233;,  Mme&#160;Bamana, M.&#160;Bentz, M.&#160;Bernhardt, Mme&#160;Delannoy, Mme&#160;Dogor-Such, M.&#160;Dussausaye, M.&#160;Florquin, Mme&#160;Lavalette, Mme&#160;Levavasseur, Mme&#160;Loir, M.&#160;M&#233;nag&#233;, Mme&#160;M&#233;lin, M.&#160;Muller, Mme&#160;Ranc et M.&#160;Tach&#233;&#160;de&#160;la&#160;Pagerie"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article unique", "articleDesignationCourte": "ART. UNIQUE", "articleDesignation": "ARTICLE UNIQUE", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0119.asp#D_Article_unique"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "9", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 9"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;9, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;L&#x2019;acc&#x00E8;s aux donn&#x00E9;es du registre national des cancers peut &#x00EA;tre autoris&#x00E9;, dans des conditions d&#x00E9;termin&#x00E9;es par d&#x00E9;cret en Conseil d&#x2019;&#x00C9;tat, &#x00E0; des fins de recherche, d&#x2019;&#x00E9;tude ou d&#x2019;&#x00E9;valuation, notamment par les organismes de recherche publique ou par les &#x00E9;tablissements de sant&#x00E9;, dans le respect des dispositions relatives &#x00E0; la protection des donn&#x00E9;es personnelles.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le registre ne doit pas &#x00EA;tre une simple base statistique passive. Cet amendement vise &#x00E0; ouvrir son potentiel &#x00E0; la recherche publique, dans le strict respect du RGPD et des r&#x00E8;gles de la CNIL. Il s&#x2019;inscrit dans la strat&#x00E9;gie nationale de lutte contre le cancer en facilitant les &#x00E9;tudes &#x00E9;pid&#x00E9;miologiques, la veille sanitaire, ou les projets innovants sur les in&#x00E9;galit&#x00E9;s d&#x2019;acc&#x00E8;s aux soins.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-06-14", "datePublication": "2025-06-18", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "RT", "libelle": "Retir\u00e9"}, "sousEtat": {"code": "RT", "libelle": "Retir\u00e9 apr\u00e8s publication"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO420120B0119P0D1N000008?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}