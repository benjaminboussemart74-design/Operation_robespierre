{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1043P0D1N000001", "chronotag": "1743111597339", "legislature": "17", "identification": {"numeroLong": "1", "numeroOrdreDepot": "1", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1043P0D1", "texteLegislatifRef": "PIONANR5L17BTC1043", "triAmendement": "poaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaae", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA607619", "groupePolitiqueRef": "PO845485", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA267763", "PA794554", "PA719286", "PA942", "PA720976", "PA267042", "PA822617", "PA795244", "PA841931", "PA793210", "PA721110"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Molac,  M.&#160;Bataille, M.&#160;Castiglione, M.&#160;Colombani, M.&#160;de&#160;Courson, M.&#160;Mathiasin, M.&#160;Favennec-B&#233;cot, Mme&#160;Froger, M.&#160;Lenormand, M.&#160;Mazaury, M.&#160;Panifous et Mme&#160;Sanquer"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 23 bis", "articleDesignationCourte": "ART. 23 BIS", "articleDesignation": "ARTICLE 23 BIS", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1043.asp#D_Article_23_bis"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "3", "alineaDesignation": "Alin\u00e9a 3"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;3, supprimer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;, sans motif l&#x00E9;gitime,&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, compl&#x00E9;ter le m&#x00EA;me alin&#x00E9;a 3 par les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;dans le but de troubler la tranquillit&#x00E9; ou le bon ordre de cet &#x00E9;tablissement&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; mieux calibrer l&#x2019;infraction d&#x2019;intrusion dans le &#x00AB;&nbsp;domaine affect&#x00E9;&nbsp;&#x00BB; &#x00E0; un &#x00E9;tablissement p&#x00E9;nitentiaire. Ce domaine n&#x2019;est pas&nbsp;clairement d&#x00E9;fini et pourrait donc recouvrir les abords imm&#x00E9;diats et la voie publique. Il est donc n&#x00E9;cessaire de pr&#x00E9;ciser que l&#x2019;infraction ne pourra&nbsp;&#x00EA;tre caract&#x00E9;ris&#x00E9;e qu&#x2019;en cas de volont&#x00E9;&nbsp;de troubler l&#x2019;ordre ou la tranquillit&#x00E9;&nbsp;de l&#x2019;&#x00E9;tablissement. La r&#x00E9;daction propos&#x00E9;e par cet amendement s&#x2019;inspire de celle pr&#x00E9;vue en mati&#x00E8;re d&#x2019;intrusion dans l&#x2019;enceinte d&#x2019;un &#x00E9;tablissement scolaire (article&nbsp;431&#x2011;22 du code p&#x00E9;nal).</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-11", "datePublication": "2025-03-18", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2025-03-27T22:39:49+01:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1043P0D1N000001?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29344", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}