{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1043P0D1N000644", "chronotag": "1743111147305", "legislature": "17", "identification": {"numeroLong": "644", "numeroOrdreDepot": "644", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1043P0D1", "texteLegislatifRef": "PIONANR5L17BTC1043", "triAmendement": "olaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaab", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA720370", "groupePolitiqueRef": "PO845407", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Mendes"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 22 bis", "articleDesignationCourte": "APR\u00c8S ART. 22 BIS", "articleDesignation": "APR\u00c8S L'ARTICLE 22 BIS, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1043.asp#D_Article_22_bis"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">L&#x2019;article&nbsp;445&#x2011;1 du code p&#x00E9;nal est compl&#x00E9;t&#x00E9; par un alin&#x00E9;a ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Lorsque les faits pr&#x00E9;vus aux deux premiers alin&#x00E9;as de ce m&#x00EA;me article sont commis en bande organis&#x00E9;e, la peine d&#x2019;emprisonnement est de 10 ans et le montant de l&#x2019;amende est de 1 000 000&nbsp;&#x20AC;.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Il s'agit ici d'aggraver le quantum de peine pour les agents priv&#x00E9;s auteurs de corruption passive ou active lorsque les faits sont commis en bande organis&#x00E9;e.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-14", "datePublication": "2025-03-20", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2025-03-27T22:32:01+01:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1043P0D1N000644?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29344", "article99": "false", "loiReference": {"codeLoi": "c p\u00e9nal", "divisionCodeLoi": "445-1"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.