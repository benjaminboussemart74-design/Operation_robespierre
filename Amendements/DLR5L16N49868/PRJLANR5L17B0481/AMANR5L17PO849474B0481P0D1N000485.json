{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO849474B0481P0D1N000485", "chronotag": "1742853087324", "legislature": "17", "identification": {"numeroLong": "CS485", "numeroOrdreDepot": "485", "prefixeOrganeExamen": "CSVIEECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B0481P0D1", "texteLegislatifRef": "PRJLANR5L17B0481", "triAmendement": "ajaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaf", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA841553", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA642988", "PA793166", "PA841605", "PA793218", "PA841837", "PA793904", "PA794590", "PA719436", "PA793852", "PA841955", "PA842097", "PA794954", "PA793382"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Tesson,  M.&#160;de&#160;L&#233;pinau, M.&#160;Dessigny, M.&#160;Golliot, M.&#160;Guitton, M.&#160;Le&#160;Bourgeois, M.&#160;Lopez-Liguori, M.&#160;Loubet, M.&#160;Meizonnet, M.&#160;Meurin, M.&#160;Renault, Mme&#160;Roy, M.&#160;Salmon et M.&#160;Tach&#233;&#160;de&#160;la&#160;Pagerie"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 2 quater", "articleDesignationCourte": "ART. 2 QUATER", "articleDesignation": "ARTICLE 2 QUATER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0481.asp#D_Article_2_quater"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer cet article.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; supprimer l&#x2019;article 2 quater, qui pr&#x00E9;voit une simplification des formalit&#x00E9;s d&#x00E9;claratives pour b&#x00E9;n&#x00E9;ficier du taux r&#x00E9;duit de TVA &#x00E0; 5,5 % sur les travaux d'am&#x00E9;lioration de la qualit&#x00E9; &#x00E9;nerg&#x00E9;tique dans des logements achev&#x00E9;s depuis plus de deux ans.</p><p style=\"text-align: justify;\">Comme &#x00E9;voqu&#x00E9; dans l&#x2019;expos&#x00E9; des motifs relatif &#x00E0; la suppression de l&#x2019;article 2 ter, la suppression des formulaires CERFA all&#x00E9;gerait certes les d&#x00E9;marches administratives des entreprises du b&#x00E2;timent, mais elle comporte des risques importants de fraude. La mention d&#x00E9;clarative seule est insuffisante pour garantir la fiabilit&#x00E9; des demandes et l&#x2019;anciennet&#x00E9; minimale du logement, fix&#x00E9;e &#x00E0; deux ans, appara&#x00EE;t trop r&#x00E9;cente pour justifier des travaux de r&#x00E9;novation.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-20", "datePublication": "2025-03-24", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-03-24T22:51:22+01:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO849474B0481P0D1N000485?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "76382", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}