{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO849474B0481P0D1N001219", "chronotag": "1743068277454", "legislature": "17", "identification": {"numeroLong": "CS1219", "numeroOrdreDepot": "1219", "prefixeOrganeExamen": "CSVIEECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B0481P0D1", "texteLegislatifRef": "PRJLANR5L17B0481", "triAmendement": "olaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaeu", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA774958", "groupePolitiqueRef": "PO845419", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA342415", "PA840939", "PA335054", "PA840765", "PA841463", "PA841107", "PA840673", "PA793816"]}, "suffixe": "et les membres du groupe Socialistes et apparent\u00e9s", "libelle": "M.&#160;Leseul,  Mme&#160;Battistel, Mme&#160;Godard, Mme&#160;Karamanli, M.&#160;Lhardit, Mme&#160;Mercier, M.&#160;Oberti, Mme&#160;Rossi, Mme&#160;Thomin et les membres du groupe Socialistes et apparent&#233;s"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 27", "articleDesignationCourte": "ART. 27", "articleDesignation": "ARTICLE 27", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0481.asp#D_Article_27"}, "amendementStandard": {"alinea": {"avant_A_Apres": "A", "numero": "9", "alineaDesignation": "Alin\u00e9a 9"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Substituer aux alin&#x00E9;as 9 et 10, l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;6&#x00B0;&nbsp;Deux d&#x00E9;put&#x00E9;s et deux s&#x00E9;nateurs, les pr&#x00E9;sidents de l&#x2019;Assembl&#x00E9;e nationale et du S&#x00E9;nat d&#x00E9;signant chacun un parlementaire issu d&#x2019;un groupe s&#x2019;&#x00E9;tant d&#x00E9;clar&#x00E9; d&#x2019;opposition&nbsp;;&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement des d&#x00E9;put&#x00E9;s Socialistes et apparent&#x00E9;s vise &#x00E0; r&#x00E9;&#x00E9;quilibrer la composition du Haut Conseil &#x00E0; la simplification pour les entreprises en portant de deux &#x00E0; quatre le nombre de parlementaires afin de permettre d&#x2019;une part, une repr&#x00E9;sentation paritaire au sein de chaque assembl&#x00E9;e et, d&#x2019;autre part, la d&#x00E9;signation d&#x2019;un d&#x00E9;put&#x00E9; et d&#x2019;un s&#x00E9;nateur issus de groupes politiques s&#x2019;&#x00E9;tant d&#x00E9;clar&#x00E9;s d&#x2019;opposition.</p><p style=\"text-align: justify;\">En outre, il permet un r&#x00E9;&#x00E9;quilibrage avec le nombre de repr&#x00E9;sentants des entreprises, d&#x2019;autant plus dans l&#x2019;hypoth&#x00E8;se de l&#x2019;adoption de notre amendement pr&#x00E9;voyant une repr&#x00E9;sentation des organisations syndicales.</p><p style=\"text-align: justify;\">&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-20", "datePublication": "2025-03-27", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-03-27T10:37:32+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO849474B0481P0D1N001219?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}