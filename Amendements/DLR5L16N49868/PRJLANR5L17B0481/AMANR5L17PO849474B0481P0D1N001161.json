{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO849474B0481P0D1N001161", "chronotag": "1742941947297", "legislature": "17", "identification": {"numeroLong": "CS1161", "numeroOrdreDepot": "1161", "prefixeOrganeExamen": "CSVIEECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B0481P0D1", "texteLegislatifRef": "PRJLANR5L17B0481", "triAmendement": "gcaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabu", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA841507", "groupePolitiqueRef": "PO845439", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA795362", "PA793980", "PA793452", "PA842045", "PA610968", "PA793780", "PA588884", "PA841701", "PA795454", "PA840997", "PA841885", "PA794008", "PA721210", "PA841351", "PA795010", "PA841419", "PA842013", "PA840947", "PA794022", "PA794146", "PA841223", "PA795636", "PA793708", "PA793664", "PA794154", "PA794778", "PA795076", "PA722142", "PA610002", "PA795808", "PA796018", "PA736201", "PA841713", "PA793948", "PA2940"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Nicolas&#160;Bonnet,  Mme&#160;Belluco, M.&#160;Fournier, M.&#160;Davi, Mme&#160;Ozenne, M.&#160;Amirshahi, Mme&#160;Arrighi, Mme&#160;Autain, Mme&#160;Balage&#160;El&#160;Mariky, M.&#160;Ben&#160;Cheikh, M.&#160;Biteau, M.&#160;Arnaud&#160;Bonnet, Mme&#160;Chatelain, M.&#160;Corbi&#232;re, M.&#160;Duplessy, Mme&#160;Garin, M.&#160;Damien&#160;Girard, M.&#160;Gustave, Mme&#160;Catherine&#160;Hervieu, M.&#160;Iordanoff, Mme&#160;Laernoes, M.&#160;Lahais, M.&#160;Lucas-Lundy, M.&#160;Peytavie, Mme&#160;Pochon, M.&#160;Raux, Mme&#160;Regol, Mme&#160;Sandrine&#160;Rousseau, M.&#160;Ruffin, Mme&#160;Sas, Mme&#160;Sebaihi, Mme&#160;Simonnet, Mme&#160;Taill&#233;-Polian, M.&#160;Tavernier, M.&#160;Thierry et Mme&#160;Voynet"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 15", "articleDesignationCourte": "ART. 15", "articleDesignation": "ARTICLE 15", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0481.asp#D_Article_15"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "6", "alineaDesignation": "Alin\u00e9a 6"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;diger ainsi l&#x2019;alin&#x00E9;a&nbsp;6&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;I&nbsp;<i>bis</i>.&nbsp;&#x2013;&nbsp;Un centre de donn&#x00E9;es peut &#x00E9;galement &#x00EA;tre qualifi&#x00E9; par d&#x00E9;cret de projet d&#x2019;int&#x00E9;r&#x00EA;t national majeur lorsqu&#x2019;il est la propri&#x00E9;t&#x00E9; des administrations, des services publics et des entit&#x00E9;s de droit priv&#x00E9; ou de droit public poursuivant une mission de service public, et qu&#x2019;il est consacr&#x00E9; &#x00E0; l&#x2019;h&#x00E9;bergement de leurs donn&#x00E9;es.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>A minima, l&#x2019;int&#x00E9;r&#x00EA;t pour la souverainet&#x00E9; nationale peut s&#x2019;appr&#x00E9;cier au regard du caract&#x00E8;re public des donn&#x00E9;es h&#x00E9;berg&#x00E9;es par les datacenters. Cet amendement propose de conditionner la possibilit&#x00E9; de d&#x00E9;cr&#x00E9;ter un datacenter projet d&#x2019;int&#x00E9;r&#x00EA;t national majeur au stockage de donn&#x00E9;es publiques, c'est-&#x00E0;-dire les donn&#x00E9;es des administrations, des services publics et des entit&#x00E9;s de droit priv&#x00E9; ou de droit public poursuivant une mission de service public administrations publiques, comme les minist&#x00E8;res, les universit&#x00E9;s, les h&#x00F4;pitaux, etc. Le seul &#x201C;int&#x00E9;r&#x00EA;t national majeur&#x201D; potentiel d&#x2019;un datacenter est de stocker des donn&#x00E9;es publiques sur le territoire national.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-20", "datePublication": "2025-03-25", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-03-25T23:31:59+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO849474B0481P0D1N001161?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}