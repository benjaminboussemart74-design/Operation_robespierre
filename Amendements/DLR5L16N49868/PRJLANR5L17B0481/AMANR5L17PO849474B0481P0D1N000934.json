{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO849474B0481P0D1N000934", "chronotag": "1743070527388", "legislature": "17", "identification": {"numeroLong": "CS934", "numeroOrdreDepot": "934", "prefixeOrganeExamen": "CSVIEECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B0481P0D1", "texteLegislatifRef": "PRJLANR5L17B0481", "triAmendement": "ouaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaf", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA795362", "groupePolitiqueRef": "PO845439", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793980", "PA841507", "PA842045", "PA793452"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Belluco,  M.&#160;Fournier, M.&#160;Nicolas&#160;Bonnet, Mme&#160;Ozenne et M.&#160;Davi"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 27 bis", "articleDesignationCourte": "APR\u00c8S ART. 27 BIS", "articleDesignation": "APR\u00c8S L'ARTICLE 27 BIS, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0481.asp#D_Article_27_bis"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Dans un d&#x00E9;lai de six mois &#x00E0; compter de la promulgation de la pr&#x00E9;sente loi, le Gouvernement remet au Parlement un rapport sur l&#x2019;impact des &#x00E9;missions hertziennes sur la sant&#x00E9; et sur les mesures &#x00E0; prendre pour r&#x00E9;duire le niveau d&#x2019;exposition de la population &#x00E0; ces &#x00E9;missions dans le cadre des objectifs de couverture en 5G de tout le territoire.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;objet de cet amendement est de demander un rapport, pour mieux &#x00E9;valuer les effets des ondes sur la sant&#x00E9;, et pour pouvoir concilier la couverture mobile de l&#x2019;ensemble du territoire, et la limitation de l&#x2019;exposition de la population aux ondes.</p><p style=\"text-align: justify;\">&nbsp;</p><p style=\"text-align: justify;\">&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-20", "datePublication": "2025-03-27", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Retir\u00e9"}}, "dateSort": "2025-03-27T11:15:06+01:00", "sort": "Retir\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO849474B0481P0D1N000934?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "zzz", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}