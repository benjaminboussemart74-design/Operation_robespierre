{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO849474B0481P0D1N001217", "chronotag": "1743008397419", "legislature": "17", "identification": {"numeroLong": "CS1217", "numeroOrdreDepot": "1217", "prefixeOrganeExamen": "CSVIEECO", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419610B0481P0D1", "texteLegislatifRef": "PRJLANR5L17B0481", "triAmendement": "ieaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaf", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA774958", "groupePolitiqueRef": "PO845419", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA342415", "PA840939", "PA335054", "PA840765", "PA841463", "PA841107", "PA840673", "PA793816"]}, "suffixe": "et les membres du groupe Socialistes et apparent\u00e9s", "libelle": "M.&#160;Leseul,  Mme&#160;Battistel, Mme&#160;Godard, Mme&#160;Karamanli, M.&#160;Lhardit, Mme&#160;Mercier, M.&#160;Oberti, Mme&#160;Rossi, Mme&#160;Thomin et les membres du groupe Socialistes et apparent&#233;s"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 17", "articleDesignationCourte": "ART. 17", "articleDesignation": "ARTICLE 17", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0481.asp#D_Article_17"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "12", "alineaDesignation": "Alin\u00e9a 12"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;l&#x2019;alin&#x00E9;a&nbsp;12, apr&#x00E8;s le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;personne&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">ins&#x00E9;rer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;&#x00E0; l&#x2019;exception de celles vis&#x00E9;es aux alin&#x00E9;as deux &#x00E0; sept de l&#x2019;article L.&nbsp;411&#x2011;2 du code de la construction et de l&#x2019;habitation,&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement des d&#x00E9;put&#x00E9;s Socialistes et apparent&#x00E9;s et travaill&#x00E9; avec l&#x2019;USH vise &#x00E0; exempter les organismes de logement social des nouvelles obligations cr&#x00E9;&#x00E9;es par ce texte.</p><p style=\"text-align: justify;\">Le pr&#x00E9;sent projet de loi pr&#x00E9;voit de conditionner la r&#x00E9;alisation de certains contrats immobiliers incluant des emplacements r&#x00E9;serv&#x00E9;s &#x00E0; des antennes de t&#x00E9;l&#x00E9;phonie &#x00E0; la production d&#x2019;une attestation d&#x2019;un op&#x00E9;rateur de t&#x00E9;l&#x00E9;phonie mobile, pourtant tiers au contrat, s&#x2019;engageant &#x00E0; exploiter ces antennes. Or, la mesure vis&#x00E9;e ne constitue pas une simplification et pourrait rendre plus difficile les transactions immobili&#x00E8;res r&#x00E9;alis&#x00E9;es par les organismes de logement social dans le cadre de leurs activit&#x00E9;s relevant des missions et services d&#x2019;int&#x00E9;r&#x00EA;t g&#x00E9;n&#x00E9;ral. Il y a donc lieu de les exempter de cette obligation.</p><p>&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-20", "datePublication": "2025-03-25", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-03-26T17:59:43+01:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO849474B0481P0D1N001217?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "3", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}