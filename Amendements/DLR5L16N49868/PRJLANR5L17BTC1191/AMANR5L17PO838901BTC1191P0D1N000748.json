, Mme&#160;Pochon, M.&#160;Raux, Mme&#160;Regol, M.&#160;Thierry, Mme&#160;Sandrine&#160;Rousseau, M.&#160;Ruffin, Mme&#160;Sas, Mme&#160;Sebaihi, Mme&#160;Simonnet, Mme&#160;Taill&#233;-Polian et M.&#160;Tavernier"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1191.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "124", "alineaDesignation": "Alin\u00e9a 124"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p>Supprimer les alin&#x00E9;as 124 et 125.&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; maintenir l'existence du&nbsp;plan d'action national pour la r&#x00E9;duction des &#x00E9;missions d'ammoniac et de protoxyde d'azote.&nbsp;</p><p style=\"text-align: justify;\">La suppression du&nbsp;plan d'action national pour la r&#x00E9;duction des &#x00E9;missions d'ammoniac et de protoxyde d'azote compromettrait gravement nos objectifs environnementaux et sanitaires en mati&#x00E8;re de pollution agricole. Ces deux gaz &#x00E0; effet de serre, issus principalement des engrais azot&#x00E9;s min&#x00E9;raux, contribuent &#x00E0; l&#x2019;acidification des sols, &#x00E0; la d&#x00E9;gradation de la biodiversit&#x00E9;, et participent au changement climatique en tant que puissants gaz &#x00E0; effet de serre.</p><p style=\"text-align: justify;\">Le plan d&#x2019;action national pr&#x00E9;vu par l&#x2019;article L. 255-1-1 du&nbsp;Code rural et de la p&#x00EA;che maritime&nbsp;est un outil essentiel pour r&#x00E9;duire ces &#x00E9;missions, r&#x00E9;duire les impacts environnementaux (pollution de&nbsp;l&#x2019;air, de l&#x2019;eau et protection de la qualit&#x00E9; des sols), pr&#x00E9;server de la sant&#x00E9; publique.</p><p style=\"text-align: justify;\">Supprimer ce plan d'action reviendrait &#x00E0; ignorer les besoins urgents de transformation du mod&#x00E8;le agricole vers une agriculture plus verte et respectueuse des &#x00E9;cosyst&#x00E8;mes.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-03", "datePublication": "2025-04-07", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2025-04-10T19:01:57+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1191P0D1N000748?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29311", "article99": "false", "loiReference": {"codeLoi": "0", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "77222", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}