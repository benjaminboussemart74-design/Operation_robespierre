M.&#160;Dive, Mme&#160;Duby-Muller, M.&#160;Forissier, M.&#160;Gosselin, Mme&#160;Gruet, M.&#160;Herbillon, M.&#160;Hetzel, M.&#160;Jeanbrun, M.&#160;Juvin, M.&#160;Le&#160;Fur, M.&#160;Lepers, M.&#160;Liger, M.&#160;Li&#233;geon, M.&#160;Marleix, Mme&#160;Alexandra&#160;Martin, Mme&#160;Fr&#233;d&#233;rique&#160;Meunier, M.&#160;Nury, M.&#160;Pauget, Mme&#160;Petex, M.&#160;Portier, M.&#160;Ray, M.&#160;Rolland, Mme&#160;Tabarot, M.&#160;Taite, M.&#160;Thi&#233;riot, M.&#160;Vermorel-Marques et M.&#160;Jean-Pierre&#160;Vigier"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 3 bis C", "articleDesignationCourte": "ART. 3 BIS C", "articleDesignation": "ARTICLE 3 BIS C", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1191.asp#D_Article_3_bis_C"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;2, substituer au mot&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;cinquante&nbsp;&#x00BB;&nbsp;</p><p style=\"text-align: justify;\">les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;deux-cent-cinquante&nbsp;&#x00BB;.&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement du groupe Droite R&#x00E9;publicaine introduit une garantie proc&#x00E9;durale au b&#x00E9;n&#x00E9;fice des tr&#x00E8;s petites entreprises et des petites et moyennes entreprises. Il vise &#x00E0; &#x00E9;viter la r&#x00E9;p&#x00E9;tition, sur une p&#x00E9;riode de douze mois, de plusieurs contr&#x00F4;les administratifs ou fiscaux portant sur les m&#x00EA;mes obligations, source d&#x2019;instabilit&#x00E9; juridique et &#x00E9;conomique pour les entreprises concern&#x00E9;es. Il maintient cependant la possibilit&#x00E9; de diligenter un nouveau contr&#x00F4;le en cas de manquement caract&#x00E9;ris&#x00E9;.</p><p style=\"text-align: justify;\">Cette mesure s&#x2019;inscrit dans l&#x2019;objectif de simplification et de s&#x00E9;curisation des relations entre l&#x2019;administration et les entreprises.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-04", "datePublication": "2025-04-08", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-04-11T23:36:37+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1191P0D1N001122?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29315", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://ww