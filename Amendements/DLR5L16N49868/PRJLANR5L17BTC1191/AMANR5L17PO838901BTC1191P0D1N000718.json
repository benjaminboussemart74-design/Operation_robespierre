{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1191P0D1N000718", "chronotag": "1757948079924", "legislature": "17", "identification": {"numeroLong": "718", "numeroOrdreDepot": "718", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1191P0D1", "texteLegislatifRef": "PRJLANR5L17BTC1191", "triAmendement": "poaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaai", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794058", "groupePolitiqueRef": "PO845425", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": "PA720644"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Gruet et M.&#160;Nury"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 26 ter", "articleDesignationCourte": "APR\u00c8S ART. 26 TER", "articleDesignation": "APR\u00c8S L'ARTICLE 26 TER, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1191.asp#D_Article_26_ter"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;article&nbsp;L.&nbsp;3322&#x2011;11 du code de la sant&#x00E9; publique, il est ins&#x00E9;r&#x00E9; un article&nbsp;L.&nbsp;3322&#x2011;12 ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;<i>Art.&nbsp;L.&nbsp;3322&#x2011;12</i>.&nbsp;&#x2013;&nbsp;La consommation d&#x2019;une boisson alcoolique de quatri&#x00E8;me et de cinqui&#x00E8;me groupe au cours d&#x2019;une visite, &#x00E0; titre on&#x00E9;reux, d&#x2019;un lieu de production d&#x2019;une telle boisson n&#x2019;est pas consid&#x00E9;r&#x00E9;e comme une vente pour consommer sur place au sens de l&#x2019;article&nbsp;L.&nbsp;3331&#x2011;1.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement vise &#x00E0; int&#x00E9;grer une disposition permettant de pr&#x00E9;ciser qu&#x2019;une visite payante de site de production ou espace mus&#x00E9;ographique d&#x00E9;di&#x00E9;s &#x00E0; l&#x2019;&#x00E9;laboration des spiritueux, incluant une d&#x00E9;gustation ne constitue pas une vente de boissons alcoolis&#x00E9;e &#x00E0; consommer sur place d&#x00E9;clenchant l&#x2019;obligation de disposer d&#x2019;une licence IV</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-03", "datePublication": "2025-06-13", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2025-06-13T23:40:29+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1191P0D1N000718?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29559", "article99": "false", "loiReference": {"codeLoi": "c sant\u00e9 publique", "divisionCodeLoi": "L3322-12"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "77880", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}