#160;Thierry, Mme&#160;Sandrine&#160;Rousseau, M.&#160;Ruffin, Mme&#160;Sas, Mme&#160;Sebaihi, Mme&#160;Simonnet, Mme&#160;Taill&#233;-Polian et M.&#160;Tavernier"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1191.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "36", "alineaDesignation": "Alin\u00e9a 36"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p>Supprimer&nbsp;les alin&#x00E9;as 36 &#x00E0; 38.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; r&#x00E9;tablir le Conseil sup&#x00E9;rieur de la for&#x00EA;t et du bois.</p><p style=\"text-align: justify;\">Le CSFB est la principale instance nationale de concertation en mati&#x00E8;re de politiques foresti&#x00E8;res. Les d&#x00E9;bats dans ses comit&#x00E9;s sp&#x00E9;cialis&#x00E9;s (Comit&#x00E9; sp&#x00E9;cialis&#x00E9; Europe et International qui a trait&#x00E9; des projets de directive Sols et de r&#x00E8;glement Surveillance des for&#x00EA;ts ; Comit&#x00E9; sp&#x00E9;cialis&#x00E9; Gestion durable qui a trait&#x00E9; des obligations l&#x00E9;gales de d&#x00E9;broussaillement, du renouvellement forestier, du plan d'action pour la pr&#x00E9;servation des sols forestiers, etc. ; Comit&#x00E9; sp&#x00E9;cialis&#x00E9; \"for&#x00EA;t, bois et territoire\"&#x2026;) sont particuli&#x00E8;rement riches.&nbsp;</p><p style=\"text-align: justify;\">A l&#x2019;heure o&#x00F9; la for&#x00EA;t fran&#x00E7;aise est &#x00E0; la fois victime et solution face au changement climatique, supprimer la principale instance de dialogue avec la soci&#x00E9;t&#x00E9; civile sur ces sujets serait un signal tr&#x00E8;s n&#x00E9;gatif.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-03", "datePublication": "2025-04-07", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-04-10T11:29:11+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1191P0D1N000646?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29310", "article99": "false", "loiReference": {"codeLoi": "0", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "77198", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}