{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO838901BTC1191P0D1N002434", "chronotag": "1757948323750", "legislature": "17", "identification": {"numeroLong": "2434", "numeroOrdreDepot": "2434", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L17PO838901BTC1191P0D1", "texteLegislatifRef": "PRJLANR5L17BTC1191", "triAmendement": "basaabaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafc", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA841769", "groupePolitiqueRef": "PO847173", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA841207", "PA840701", "PA840721", "PA841395", "PA840983", "PA840809", "PA793852", "PA794946"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Michoux,  M.&#160;Alloncle, M.&#160;Chaix, M.&#160;Chavent, M.&#160;Michelet, M.&#160;Lenoir, M.&#160;Verny, M.&#160;Meurin et M.&#160;Villedieu"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 15", "articleDesignationCourte": "ART. 15", "articleDesignation": "ARTICLE 15", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/1191.asp#D_Article_15"}, "amendementStandard": {"alinea": {"avant_A_Apres": "A", "numero": "40", "alineaDesignation": "Alin\u00e9a 40"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;40, r&#x00E9;tablir le 1&#x00B0;&nbsp;dans la r&#x00E9;daction suivante&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;1&#x00B0;&nbsp;Apr&#x00E8;s le 6&#x00B0;&nbsp;du III, il est ins&#x00E9;r&#x00E9; un 6 <i>bis</i> ainsi r&#x00E9;dig&#x00E9;&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;&#x00AB;&nbsp;6 <i>bis</i> Pour la tranche mentionn&#x00E9;e au 2&#x00B0;&nbsp;du pr&#x00E9;sent III, un espace naturel ou agricole utilis&#x00E9; pour implanter de nouvelles installations class&#x00E9;es pour la protection de l&#x2019;environnement ou &#x00E9;tendre de telles installations n&#x2019;est pas comptabilis&#x00E9; dans la consommation d&#x2019;espaces naturels, agricoles et forestiers ;&nbsp;&#x00BB;&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p>Cet amendement vise &#x00E0; sortir les sites industriels soumis au r&#x00E9;gime des installations class&#x00E9;es pour la protection de l&#x2019;environnement (ICPE) du d&#x00E9;compte de l&#x2019;artificialisation.</p><p>Ce dispositif permet&nbsp;de favoriser la r&#x00E9;industrialisation de la France et pour favoriser l'&#x00E9;mergence de nouvelles industrie.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-04", "datePublication": "2025-05-28", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-05-28T15:41:59+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO838901BTC1191P0D1N002434?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L17S2025IDS29461", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "29962", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "77032", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}