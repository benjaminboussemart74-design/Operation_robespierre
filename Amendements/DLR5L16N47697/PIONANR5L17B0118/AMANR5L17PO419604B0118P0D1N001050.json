{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419604B0118P0D1N001050", "chronotag": "1757947120111", "legislature": "17", "identification": {"numeroLong": "AC1050", "numeroOrdreDepot": "1050", "prefixeOrganeExamen": "CION-CEDU", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419604B0118P0D1", "texteLegislatifRef": "PIONANR5L17B0118", "triAmendement": "dcaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaau", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "Rapporteur", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793398", "groupePolitiqueRef": "PO845470", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": "PA608826"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Patrier-Leitus, rapporteur et Mme&#160;Duby-Muller, rapporteure"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 7", "articleDesignationCourte": "ART. 7", "articleDesignation": "ARTICLE 7", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0118.asp#D_Article_7"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "4", "alineaDesignation": "Alin\u00e9a 4"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;diger ainsi les deux premi&#x00E8;res phrases de l&#x2019;alin&#x00E9;a&nbsp;4&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;L&#x2019;ensemble des biens, des droits, des obligations, des contrats, des conventions, des accr&#x00E9;ditations, des habilitations et des autorisations de toute nature de l&#x2019;&#x00E9;tablissement public Institut national de l&#x2019;audiovisuel, en France et hors de France, sont de plein droit ceux de la soci&#x00E9;t&#x00E9; anonyme Institut national de l&#x2019;audiovisuel &#x00E0; la date de sa transformation. La transformation de forme sociale n&#x2019;a aucune incidence sur les biens, les droits, les obligations, les contrats, les conventions, les accr&#x00E9;ditations, les habilitations et les autorisations conclus par l&#x2019;Institut national de l&#x2019;audiovisuel.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;INA &#x00E9;tant habilit&#x00E9; &#x00E0; d&#x00E9;livrer des dipl&#x00F4;mes au titre de ses activit&#x00E9;s de formation initiale, il convient d&#x2019;ajouter sa capacit&#x00E9; &#x00E0; dipl&#x00F4;mer &#x00E0; la liste des pr&#x00E9;rogatives transmises de plein droit et sans incidence ou modification, par l&#x2019;Epic &#x00E0; la SA INA.</p><p style=\"text-align: justify;\">Les modifications r&#x00E9;glementaires n&#x00E9;cessaires, notamment &#x00E0; l&#x2019;article D.75&#x2011;10 du Code de l&#x2019;&#x00E9;ducation, pourront ensuite &#x00EA;tre prises sur la base de la loi ainsi compl&#x00E9;t&#x00E9;e.</p><p style=\"text-align: justify;\">L'amendement proc&#x00E8;de &#x00E9;galement &#x00E0; plusieurs corrections r&#x00E9;dactionnelles. </p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-31", "datePublication": "2025-06-17", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2025-06-17T23:24:54+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419604B0118P0D1N001050?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "RG", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}