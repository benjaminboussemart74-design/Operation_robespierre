{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419604B0118P0D1N000129", "chronotag": "1757947324277", "legislature": "17", "identification": {"numeroLong": "AC129", "numeroOrdreDepot": "129", "prefixeOrganeExamen": "CION-CEDU", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419604B0118P0D1", "texteLegislatifRef": "PIONANR5L17B0118", "triAmendement": "kiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaao", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794562", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA796118", "PA793158", "PA720822", "PA794198", "PA840159", "PA795644", "PA793362", "PA841749", "PA840869", "PA795552", "PA794670", "PA841199", "PA793656", "PA842255"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Ballard,  Mme&#160;Auzanot, M.&#160;Beaurain, M.&#160;Bilde, M.&#160;Chudeau, M.&#160;Clavet, Mme&#160;Da&#160;Conceicao&#160;Carvalho, M.&#160;Gonzalez, Mme&#160;Joncour, Mme&#160;Joubert, M.&#160;Odoul, Mme&#160;Parmentier, M.&#160;Perez, Mme&#160;Pollet et Mme&#160;Sicard"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 12", "articleDesignationCourte": "ART. 12", "articleDesignation": "ARTICLE 12", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0118.asp#D_Article_12"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "4", "alineaDesignation": "Alin\u00e9a 4"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;4, supprimer les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;ne porte pas atteinte &#x00E0; l&#x2019;imp&#x00E9;ratif fondamental de pluralisme et &#x00E0; l&#x2019;int&#x00E9;r&#x00EA;t du public et qu&#x2019;elle&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La loi n&#x00B0;&nbsp;86&#x2011;1067 du 30 septembre 1986 et notamment cet article&nbsp;42&#x2011;3 disposent d&#x00E9;j&#x00E0; de nombreux niveaux de contr&#x00F4;le concernant le pluralisme.<br><br></p><p style=\"text-align: justify;\">Il n&#x2019;est pas indispensable selon nous de rajouter encore une contrainte de pluralisme &#x00E0; l&#x2019;alin&#x00E9;a&nbsp;4 de l&#x2019;article&nbsp;42&#x2011;3 d&#x2019;autant plus que cet article&nbsp;12 a vocation &#x00E0; assouplir les r&#x00E8;gles anti concentration auxquelles font face les acteurs de l&#x2019;audiovisuel fran&#x00E7;ais.<br><br></p><p style=\"text-align: justify;\">Il serait donc louable, dans une optique d&#x2019;efficacit&#x00E9; des rapprochements pour les acteurs du secteur, de limiter le p&#x00E9;rim&#x00E8;tre de cet alin&#x00E9;a&nbsp;4 &#x00E0; la pr&#x00E9;vention d&#x2019;&#x00E9;ventuelles d&#x00E9;rives sp&#x00E9;culatives.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-27", "datePublication": "2025-06-18", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-06-18T09:44:34+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419604B0118P0D1N000129?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}