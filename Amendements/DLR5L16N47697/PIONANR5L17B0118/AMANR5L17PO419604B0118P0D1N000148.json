{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L17PO419604B0118P0D1N000148", "chronotag": "1757947195509", "legislature": "17", "identification": {"numeroLong": "AC148", "numeroOrdreDepot": "148", "prefixeOrganeExamen": "CION-CEDU", "numeroRect": "0"}, "examenRef": "EXANR5L17PO419604B0118P0D1", "texteLegislatifRef": "PIONANR5L17B0118", "triAmendement": "buaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaugi", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794562", "groupePolitiqueRef": "PO845401", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA796118", "PA793158", "PA720822", "PA794198", "PA840159", "PA795644", "PA793362", "PA841749", "PA840869", "PA795552", "PA794670", "PA841199", "PA793656", "PA842255"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Ballard,  Mme&#160;Auzanot, M.&#160;Beaurain, M.&#160;Bilde, M.&#160;Chudeau, M.&#160;Clavet, Mme&#160;Da&#160;Conceicao&#160;Carvalho, M.&#160;Gonzalez, Mme&#160;Joncour, Mme&#160;Joubert, M.&#160;Odoul, Mme&#160;Parmentier, M.&#160;Perez, Mme&#160;Pollet et Mme&#160;Sicard"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 5", "articleDesignationCourte": "ART. 5", "articleDesignation": "ARTICLE 5", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/17/textes/0118.asp#D_Article_5"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "11", "alineaDesignation": "Alin\u00e9a 11"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;11, supprimer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;, France M&#x00E9;dias Monde&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Ce pr&#x00E9;sent amendement vise &#x00E0; retirer France M&#x00E9;dias Monde de la holding &#x00AB;&nbsp;France M&#x00E9;dias&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">il est important selon nous d&#x2019;exclure l&#x2019;audiovisuel ext&#x00E9;rieur de cet holding par coh&#x00E9;rence avec notre&nbsp;projet&nbsp;de privatisation &#x00E0; terme d&#x2019;une partie de l&#x2019;audiovisuel public. Celui-ci via les m&#x00E9;dias du groupe public &#x00AB;&nbsp;France M&#x00E9;dias Monde&nbsp;&#x00BB;, repr&#x00E9;sentent la voix de la France dans le monde et jouent un r&#x00F4;le d&#x2019;influence important.</p><p style=\"text-align: justify;\">Dans un contexte international tendu, o&#x00F9; la guerre informationnelle menace les int&#x00E9;r&#x00EA;ts de la France &#x00E0; travers le monde, les m&#x00E9;dias de service public diffusant &#x00E0; l&#x2019;&#x00E9;tranger ne se contentent pas seulement de d&#x00E9;fendre la francophonie et notre culture, mais participent &#x00E9;galement au rayonnement de notre pays et &#x00E0; la d&#x00E9;fense de son image.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-03-27", "datePublication": "2025-06-17", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2025-06-17T22:30:33+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419604B0118P0D1N000148?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "76722", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}