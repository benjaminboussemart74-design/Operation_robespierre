}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;l&#x2019;alin&#x00E9;a&nbsp;4, apr&#x00E8;s le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;conventions&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">ins&#x00E9;rer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;,&nbsp;qui sont rendues publiques d&#x00E8;s leur signature,&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent sous-amendement vise &#x00E0; renforcer la transparence et l&#x2019;acc&#x00E8;s &#x00E0; l&#x2019;information en pr&#x00E9;cisant que les conventions&nbsp;strat&#x00E9;giques pluriannuelles sont rendues publiques apr&#x00E8;s leur signature afin de garantir une meilleure information des citoyens et des parties prenantes sur les engagements pris par la soci&#x00E9;t&#x00E9; France M&#x00E9;dias dans le cadre de sa mission de service public.&nbsp;Il s&#x2019;inscrit ainsi dans une d&#x00E9;marche de transparence et de contr&#x00F4;le d&#x00E9;mocratique et permettra de&nbsp;renforcer la confiance du public dans la gestion et l&#x2019;ex&#x00E9;cution des missions de l&#x2019;audiovisuel public, en garantissant un acc&#x00E8;s clair aux modalit&#x00E9;s et objectifs fix&#x00E9;s dans ces conventions.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2025-04-08", "datePublication": "2025-06-17", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Retir\u00e9"}}, "dateSort": "2025-06-17T21:36:03+02:00", "sort": "Retir\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L17PO419604B0118P0D1N002041?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "4", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "31842", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "79998", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}